@page "/"
@using Core

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

@{
    foreach (var movie in movies)
    {
        <div>
            <h2>@movie.Title</h2>
            <p>Score: @movie.Score</p>
        </div>
    }
}

<SurveyPrompt Title="How is Blazor working for you?"/>

@code {
    private List<Movie> movies = FakeData.Movies;
}